<h2>Show contact</h2>

<ul>

  <li>
    <strong>First name:</strong>
    <%= @contact.first_name %>
  </li>

  <li>
    <strong>Last name:</strong>
    <%= @contact.last_name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @contact.email %>
  </li>

</ul>

<%= link "Edit", to: contact_path(@conn, :edit, @contact) %>
<%= link "Back", to: contact_path(@conn, :index) %>
